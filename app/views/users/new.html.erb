<h1>User#new</h1>
<%= form_for(@user) do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

   <p>
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </p>

  <%= f.label :password_confirmation %><br>
  <%= f.text_field :password_confirmation %>

  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username %>
  </p>

  <p>
    <%= f.label :phone %><br>
    <%= f.text_field :phone %>
  </p>

  <p>
    <%= f.label :photo %> <br/>
    <%= f.file_field :photo %>
  </p> 
  
  <p>
    <%= f.submit :class => 'btn btn-default' %>
  </p>
<% end %>
