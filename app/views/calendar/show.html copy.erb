<h1>Listing Events</h1>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Title</th>
      <th>Notes</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.notes %></td>
        <td><%= event.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

